{
  "name": "quasar-skeleton",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "chf <hi.phone.chan@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js './src*/**/*.{js,cjs,mjs,vue}'",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "cross-env APP_ENV=development quasar dev",
    "dev:android": "cross-env APP_ENV=development quasar dev -m capacitor -T android",
    "dev:ios": "cross-env APP_ENV=development quasar dev -m capacitor -T ios",
    "build": "cross-env APP_ENV=production quasar build",
    "build:android": "cross-env APP_ENV=production quasar build -m capacitor -T android",
    "build:ios": "cross-env APP_ENV=production quasar build -m capacitor -T ios",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.15",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.8.4",
    "default-passive-events": "^2.0.0",
    "path-browserify": "^1.0.1",
    "pinia": "^3.0.1",
    "quasar": "^2.16.2",
    "vue": "^3.5.13",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.2.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.16",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-mock": "^2.9.6"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
